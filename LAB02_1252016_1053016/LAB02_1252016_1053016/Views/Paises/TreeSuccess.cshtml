@model List<BinTree.Nodo<LAB02_1252016_1053016.Models.Pais>>

@{
    Layout = null;
}

<h2>Lista de Paises</h2>

<table cellpadding="2" cellspacing="2" border="1">
    <tr>
        <th>Nombre Pais</th>
        <th>Apellido</th>
    </tr>

    @for (int i = 0; i < Model.Count; i++)
    {
        <tr>
            <td>@Model.ElementAt(i).Value.NombrePais </td>
            <td>@Model.ElementAt(i).Value.Grupo </td>
            <td>@Html.ActionLink("Delete", "EditarPais", new { position = Model.ElementAt(i) })</td>
        </tr>
    }
</table>
@Html.ActionLink("Back to Menu", "Paises")
