@model List<BinTree.Nodo<string>>

@{
    Layout = null;
}

<h2>Lista de cadenas</h2>

<table cellpadding="2" cellspacing="2" border="1">
    <tr>
        <th>Data</th>
    </tr>

    @for (int i = 0; i < Model.Count; i++)
    {
        <tr>
            <td>@Model.ElementAt(i).Value </td>
            <td>@Html.ActionLink("Delete", "DeleteString", new { position = Model.ElementAt(i) })</td>
        </tr>
    }
</table>
@Html.ActionLink("Back to Menu", "Cadenas")